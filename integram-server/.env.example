# Integram Docker Environment Configuration

# Database Configuration
DB_HOST=mysql
DB_NAME=integram_db
DB_USER=integram_user
DB_PASSWORD=integram_pass
DB_ROOT_PASSWORD=integram_root_pass

# Application Settings
APP_ENV=production
APP_DEBUG=false
APP_URL=http://localhost:8080

# Ports
HTTP_PORT=8080
HTTPS_PORT=8443

# MySQL Settings
MYSQL_PORT=3306

# Node.js API (если используется)
NODE_PORT=3001
NODE_ENV=production

# Timezone
TZ=Europe/Moscow

# Backup Settings
BACKUP_PATH=./backups
